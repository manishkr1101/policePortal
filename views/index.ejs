  <section id="services" class="section parallax dark">
    <div class="container">
      <div class="row">
        <div class="span12">
          <div class="heading">
            <h3><span>Our services</span></h3>
          </div>
          <div class="sub-heading">
            <p>
              Gangs show there colors so we show ours red and blues all the way.
            </p>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="span3">
          <div class="box aligncenter">
            <div class="halftop">
              <h4><strong>FIR Management</strong></h4>
              <!-- <i class="icon-desktop icon-4x"></i>-->
              <img src="images/fir.jpg" width="55px" height="55px" />
            </div>
            <p>
              View and manage recorded FIRs/ New FIRs
            </p>
            <a href="/fir" class="btn btn-theme">View</a>
          </div>
        </div>
        <div class="span3">
          <div class="box aligncenter">
            <div class="halftop">
              <h4><strong>Criminal Records</strong></h4>
              <!--<i class="icon-briefcase icon-4x"></i>-->
              <img src="images/download.jpg" width="51px" height="51px" />
            </div>
            <p>
              View/Manage records of added criminals/cases
            </p>
            <a href="/criminal" class="btn btn-theme">View</a>
          </div>
        </div>
        <div class="span3">
          <div class="box aligncenter">
            <div class="halftop">
              <h4><strong>Complaint Management</strong></h4>
              <img src="images/complain.jpg" width="55px" height="55px" />
              <!-- <i class="icon-mobile-phone icon-4x"></i>-->
            </div>
            <p>
              View and Manage complaints recorded/added
            </p>
            <a href="/complaint" class="btn btn-theme">View</a>
          </div>
        </div>
        <% if(typeof ps.admin != 'undefined' && ps.admin == 1){ %>
          <div class="span3">
            <div class="box aligncenter">
              <div class="halftop">
                <h4><strong>Fund Allocation</strong></h4>
