                <!-- <i class="icon-mobile-phone icon-4x"></i>-->
              </div>
              <p>
                Verify requests for Funds and allocate accordingly
              </p>
              <a href="/fundAllocation" class="btn btn-theme">View</a>
            </div>
          </div>
        <% } %>
        
      </div>
    </div>
  </section>
