<%- include('partials/header') %>

<section>
  <div class="header">
    <h1>List of Lost Vehicles</h1>
  </div>
  <div class="inputs container">
    <div class="row">
      <form action="" method="post" enctype="multipart/form-data">
        <div class="name col s4">
          <div class="input-field">
            <i class="material-icons prefix">search</i>
            <input
              placeholder="Search by Vehicle number"
              id="query"
              type="search"
              class="validate"
            />
          </div>
        </div>
        <div class="col s2">
          <button
            class="btn waves-effect waves-light submit-btn"
            type="submit"
            name=""
          >
            Search
            <i class="material-icons right">arrow_forward</i>
          </button>
        </div>
      </form>
    </div>
  </div>
  <div class="lost-vehicle-list">
    <div class="row">
      <table class="striped highlight">
        <thead>
          <tr>
            <th>S.No.</th>
            <th>Regisratoin no.</th>
            <th>Engine no.</th>
            <th>Chassis no.</th>
            <th>Verify</th>
          </tr>
        </thead>

        <tbody>
          <tr>
            <td></td>
            <td>Regisratoin no.</td>
            <td>Engine no.</td>
            <td>Chassis no.</td>
            <td>
              <p>
                <label>
                  <input class="with-gap" name="group1" type="radio" />
                  <span>Accept</span>
                </label>
                <label>
                  <input class="with-gap" name="group1" type="radio" />
                  <span>Reject</span>
                </label>
              </p>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</section>

<%- include('partials/footer') %>
