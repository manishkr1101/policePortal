<%- include('partials/header')%>
<section>
  <div class="header">
    <h2>Vehicle Details</h2>
  </div>
  <div class="container">
    <div class="row">
      <fieldset>
        <legend>
          <h5>Vehicle-Particulars</h5>
        </legend>
        <table>
          <tr>
            <td>Registraton Number:</td>
            <td><%= vehicle['vehicle_regno']%></td>
            <td>Engine Number:</td>
            <td><%= vehicle['vehicle_engineno']%></td>
          </tr>
          <tr>
            <td>Chassis Number:</td>
            <td><%= vehicle['vehicle_chasisno']%></td>
          </tr>
        </table>
      </fieldset>
      <fieldset>
        <legend>
          <h5>Owner Details</h5>
        </legend>
        <table>
          <tr>
            <td>Details:</td>
            <td><img src="/images/download.jpg" /></td>
          </tr>
        </table>
      </fieldset>
    </div>
  </div>
</section>

<%- include('partials/footer')%>
